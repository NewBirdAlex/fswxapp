/*  popup  */

.popup-dialog__mask {
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      z-index: 10;
      background: rgba(0, 0, 0, 0.4);
      display: none;
}

.popup-dialog__container {
      position: fixed;
      bottom: 0;
      width: 100%;
      left: 0;
      background: white;
      transform: translateY(150%);
      transition: all 0.4s ease;
      z-index: 12;
      height: 70%;
}

.popup-dialog_spec_container {
      position: fixed;
      bottom: 0;
      width: 100%;
      left: 0;
      background: rgba(255, 255, 255, 0.98);
      transform: translateY(150%);
      transition: all 0.4s ease;
      z-index: 12;
      /* min-height: 680rpx; */
      max-height: 75%;
}

.popup-dialog--show .popup-dialog__container,
.popup-dialog--show .popup-dialog_spec_container,
.popup-dialog-pi--show .popup-dialog__container {
      transform: translateY(0);
}

.popup-dialog--show .popup-dialog__mask,
.popup-dialog-pi--show .popup-dialog__mask {
      display: block;
}

