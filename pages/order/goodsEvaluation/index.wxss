page {
  background: #f7f7f7;
}

.good-contain {
  width:100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding: 22rpx;
  background: #fff;
  box-sizing: border-box;
}
.good-contain:not(:first-child){
  margin-top:20rpx;
}
.good-img {
  width: 140rpx;
  height: 140rpx;
}
.good-right{
  flex:1;
  width:60%;
}
.good-tit {
  height:40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #4a4a4a;
  margin-left: 20rpx;
}

.star {
  width: 30rpx;
  height: 30rpx;

}

.star-contain:first-child {
  margin-left: 20rpx;
}
.star-good{
  display: flex;

}
.star-contain{
  width:30rpx;
  height:30rpx;
  margin-left:10rpx;
}
.eva {
  width: 100%;
  height: 240rpx;
  background: #f7f7f7;
  padding: 20rpx;
  box-sizing: border-box;
}

.pla {
  font-size: 24rpx;
}

.eva-content {
  font-size: 24rpx;
}

textarea {
  width: 100%;
  height: 200rpx;
}

.add {
  width: 160rpx;
  height: 160rpx;
  border: 1rpx dashed #b9b9b9;
  display: flex; 
  flex-direction: column;
  justify-content: center;
  align-items: center; 
  background: #fff;
}

.add-camera {
  width: 54rpx;
  height: 44rpx;
  display: block;
}

.add-img {
  width: 160rpx;
  height: 160rpx;
  margin-right: 20rpx;
 
}

.add-word {
  font-size: 22rpx;
  color: #9b9b9b;
  margin-top: 10rpx;
}

.add-part {
  background: #fff;
  padding: 30rpx 25rpx;
  box-sizing: border-box;
  display: flex;
}
.add-all{
  display: inline-block;
}
.check-contain {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx 30rpx 20rpx;
  background: #fff;
}

.check {
  width: 45rpx;
  height: 45rpx;
}

.check-word {
  font-size: 24rpx;
  color: #4a4a4a;
  margin-left: 15rpx;
}

.delete {
  width: 35rpx;
  height: 35rpx;
  background: #000;
  opacity: 0.7;
  border-radius: 100%;
  position: absolute;
  text-align: center;
  line-height: 28rpx;
  margin-left: 18%;
  margin-top: -2%;
}

.add-close {
  width: 20rpx;
  height: 20rpx;
}

.score {
  font-size: 28rpx;
  color: #4a4a4a;
  padding-top: 50rpx;
  padding-bottom: 20rpx;
  padding-left: 20rpx;
  background: #fff;
  border-bottom: 1rpx solid #e4e4e4;
}

.score-part {
  background: #fff;
  padding-top: 40rpx;
  padding-left: 22rpx;
  font-size: 24rpx;
  color: #4a4a4a;
  display: flex;
  flex-direction: row;
}

.star-two {
  width: 36rpx;
  height: 36rpx;
  margin-left: 20rpx;
}
.last-dev{
  background: #fff;
  height:50rpx;
  padding-top:40rpx;
  padding-bottom:60rpx; 
}
.dev {
  width: 141rpx;
  height: 50rpx;
  border: 1rpx solid #f93473;
  font-size: 24rpx;
  color: #f93473;
  line-height: 50rpx;
  text-align: center;
  border-radius: 5rpx;
  position: absolute;
  right:30rpx;
}
.good-contain:not(:first-child){
margin-top:20rpx;
}